{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "booking": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/booking-app-starter",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                "inject": true
              },
              {
                "input": "node_modules/font-awesome/css/font-awesome.min.css",
                "inject": true
              },
              {
                "input": "node_modules/admin-lte/bootstrap/css/bootstrap.min.css",
                "inject": true
              },
              {
                "input": "node_modules/admin-lte/dist/css/AdminLTE.min.css",
                "inject": true
              },
              {
                "input": "node_modules/admin-lte/dist/css/skins/skin-green.min.css",
                "inject": true
              },
              {
                "input": "node_modules/admin-lte/plugins/iCheck/flat/green.css",
                "inject": true
              },
              {
                "input": "src/styles.css",
                "inject": true
              }
            ],
            "scripts": [
              {
                "input": "node_modules/jquery/dist/jquery.min.js",
                "inject": true
              },
              {
                "input": "node_modules/admin-lte/bootstrap/js/bootstrap.min.js",
                "inject": true
              },
              {
                "input": "node_modules/admin-lte/dist/js/app.min.js",
                "inject": true
              }
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "src": "src/environments/environment.ts",
                  "replaceWith": "src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "booking:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "booking:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "booking:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              {
                "input": "node_modules/jquery/dist/jquery.min.js",
                "inject": true
              },
              {
                "input": "node_modules/admin-lte/bootstrap/js/bootstrap.min.js",
                "inject": true
              },
              {
                "input": "node_modules/admin-lte/dist/js/app.min.js",
                "inject": true
              }
            ],
            "styles": [
              {
                "input": "node_modules/font-awesome/css/font-awesome.min.css",
                "inject": true
              },
              {
                "input": "node_modules/admin-lte/bootstrap/css/bootstrap.min.css",
                "inject": true
              },
              {
                "input": "node_modules/admin-lte/dist/css/AdminLTE.min.css",
                "inject": true
              },
              {
                "input": "node_modules/admin-lte/dist/css/skins/skin-green.min.css",
                "inject": true
              },
              {
                "input": "node_modules/admin-lte/plugins/iCheck/flat/green.css",
                "inject": true
              },
              {
                "input": "src/styles.css",
                "inject": true
              }
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        }
      }
    },
    "booking-e2e": {
      "root": "e2e",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "booking:serve"
          }
        }
      }
    }
  },
  
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "bkn",
      "style": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "bkn"
    }
  },
  "cli": {
    "analytics": false
  }
}